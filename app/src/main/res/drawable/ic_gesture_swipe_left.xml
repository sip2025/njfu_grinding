<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="120dp"
    android:viewportWidth="120"
    android:viewportHeight="120">
    
    <!-- 手指图标 -->
    <group android:name="finger">
        <path
            android:name="fingerPath"
            android:fillColor="#4CAF50"
            android:pathData="M 70,60 
                           Q 70,50 65,45 
                           Q 60,40 50,40 
                           L 50,30 
                           Q 50,20 45,15 
                           Q 40,10 35,10 
                           Q 30,10 25,15 
                           Q 20,20 20,30 
                           L 20,60 
                           Q 20,70 25,75 
                           Q 30,80 40,80 
                           L 60,80 
                           Q 70,80 70,70 Z"/>
    </group>
    
    <!-- 箭头 -->
    <group android:name="arrow">
        <path
            android:name="arrowPath"
            android:strokeColor="#2196F3"
            android:strokeWidth="3"
            android:strokeLineCap="round"
            android:pathData="M 100,60 L 50,60 M 60,50 L 50,60 L 60,70"/>
    </group>
</vector>