<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="120dp"
    android:viewportWidth="120"
    android:viewportHeight="120">
    
    <!-- 手指图标 -->
    <group android:name="finger">
        <path
            android:name="fingerPath"
            android:fillColor="#4CAF50"
            android:pathData="M 30,60 
                           Q 30,50 35,45 
                           Q 40,40 50,40 
                           L 50,30 
                           Q 50,20 55,15 
                           Q 60,10 65,10 
                           Q 70,10 75,15 
                           Q 80,20 80,30 
                           L 80,60 
                           Q 80,70 75,75 
                           Q 70,80 60,80 
                           L 40,80 
                           Q 30,80 30,70 Z"/>
    </group>
    
    <!-- 箭头 -->
    <group android:name="arrow">
        <path
            android:name="arrowPath"
            android:strokeColor="#2196F3"
            android:strokeWidth="3"
            android:strokeLineCap="round"
            android:pathData="M 20,60 L 70,60 M 60,50 L 70,60 L 60,70"/>
    </group>
</vector>